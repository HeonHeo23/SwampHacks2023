 : task);\r\n    setTasks(listTasks);<TaskForm/>\r\n    localStorage.setTasks(\"tasksList\",JSON.stringify(listTasks));\r\n  }\r\n  \r\n  const handleDelete = (id) => {\r\n    const listTasks = tasks.filter((task) => task.id !== id);\r\n    setTasks(listTasks);\r\n    localStorage.setTasks(\"tasksList\",JSON.stringify(listTasks));\r\n  }\r\n\r\n  return (\r\n    <div>\r\n        <TaskList\r\n          tasks={tasks}\r\n          handleCheck={handleCheck}\r\n          handleDelete={handleDelete} \r\n        />\r\n        <TaskForm tasks={tasks} setTasks={setTasks}/>\r\n\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default MakeGroup","C:\\Users\\maxim\\OneDrive\\Documents\\GitHub\\SwampHacks2023\\SwampHacks2023\\src\\new\\Room\\Room.js",["94","95","96","97"],[],"C:\\Users\\maxim\\OneDrive\\Documents\\GitHub\\SwampHacks2023\\SwampHacks2023\\src\\new\\common\\nav.js",["98"],[],"C:\\Users\\maxim\\OneDrive\\Documents\\GitHub\\SwampHacks2023\\SwampHacks2023\\src\\new\\Home\\Home.js",["99","100"],[],"import React from 'react'\r\nimport axios from \"axios\";\r\nimport {useEffect, useState} from \"react\";\r\n\r\nconst client = axios.create({\r\n  baseURL: \"http://localhost:3500/rooms\" \r\n});\r\n\r\nconst Home = () => {\r\n  const [name, setName] = useState(localStorage.getItem(\"name\"));\r\n  const [rooms, setRooms] = useState([]);\r\n  const [flag, setFlag] = useState(0);\r\n\r\n  useEffect(() => {\r\n    client.get()\r\n    .then((res) => {\r\n      setRooms(res.data);\r\n    })\r\n  }, [flag]); //no dependency here allows commenting of request result actions\r\n     //add time-polling solution here\r\n\r\n  function createRoom(name, pass) {\r\n    client.post(\"\", {\r\n      id: rooms.length === 0 ? 0 : rooms[rooms.length-1].id + 1,\r\n      name: name,\r\n      password: pass, \r\n      users: [localStorage.getItem(\"name\")] //MAKE THIS MORE SECURE\r\n    })\r\n    .then((res) => {\r\n      //setRooms([...rooms, res.data]);\r\n      setFlag(flag + 1);\r\n    })  \r\n  }\r\n\r\n  function updateRoomUser(paramId, user) {\r\n    const room = rooms.find(room => room.id === paramId);\r\n    const userIndex = room.users.indexOf(user);\r\n    client.put(`/${paramId}`, {\r\n      id: paramId,\r\n      name: room.name,\r\n      password: room.password,\r\n      users: userIndex === -1 ? room.users.concat([user]) : room.users.filter((u) => u !== user)\r\n    })\r\n    .then((res) => {\r\n      //const myRoom = rooms.findIndex(room => room.id === paramId)\r\n      setFlag(flag + 1);\r\n    })\r\n  }\r\n\r\n  function deleteRoom(name, pass) {\r\n    const room = rooms.find(room => room.name === name && room.password === pass && room.users[0] === localStorage.getItem(\"name\"))\r\n    client.delete(`/${room.id}`)\r\n    .then(() => {\r\n      //setRooms(null);\r\n      setFlag(flag + 1);\r\n    })\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <p>Hello, {name}</p>\r\n      <input type=\"text\" id=\"nameBox\"/>\r\n      <button onClick={() => {\r\n        localStorage.setItem(\"name\", document.getElementById(\"nameBox\").value);\r\n        setName(localStorage.getItem(\"name\"));\r\n      }}>Submit Name</button>\r\n\r\n      <p>Your rooms:</p>\r\n      <ul>\r\n        {rooms.map((room) => (\r\n          room.users.indexOf(localStorage.getItem(\"name\")) >= 0 ?\r\n          <>\r\n            <h1 style={{display: 'inline'}}>{room.name}</h1>\r\n            <button style={{display: 'inline', marginLeft: \"100px\"}}\r\n              onClick={() => {\r\n                updateRoomUser(room.id, localStorage.getItem(\"name\"));\r\n                }\r\n              }\r\n            >Leave room</button>\r\n          </>\r\n          : <></>\r\n        ))}\r\n      </ul>\r\n      {\r\n        (rooms.length === 0) ? <p>No rooms!</p> : <></>\r\n      }\r\n      <p>Other rooms:</p>\r\n      <ul>\r\n        {rooms.map((room) => (\r\n          room.users.indexOf(localStorage.getItem(\"name\")) < 0 ?\r\n          <>\r\n            <h1 style={{display: 'inline'}}>{room.name}</h1>\r\n            <button style={{display: 'inline', marginLeft: \"100px\"}}\r\n              onClick={() => {\r\n                updateRoomUser(room.id, localStorage.getItem(\"name\"));\r\n                }\r\n              }\r\n            >Join room</button>\r\n            <p></p>\r\n          </>\r\n          : <></>\r\n        ))}\r\n      </ul>\r\n      {/* <input type=\"text\" id=\"roomBox\" placeholder='Room name'/>\r\n      <input type=\"text\" id=\"passBox\" placeholder='Room password'/>\r\n      <button onClick={() => createRoom(document.getElementById(\"roomBox\").value, document.getElementById(\"passBox\").value)}>Create room</button>\r\n      <button onClick={() => deleteRoom(document.getElementById(\"roomBox\").value, document.getElementById(\"passBox\").value)}>Delete Room</button> */}\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Home","C:\\Users\\maxim\\OneDrive\\Documents\\GitHub\\SwampHacks2023\\SwampHacks2023\\src\\new\\Room\\Goal.js",[],[],"C:\\Users\\maxim\\OneDrive\\Documents\\GitHub\\SwampHacks2023\\SwampHacks2023\\src\\new\\TaskList\\TaskList.js",["101","102","103"],[],"import React from 'react'\r\nimport './TaskList.css'\r\nimport {FaTrashAlt} from 'react-icons/fa';\r\nimport Task from '../Task/Task.js';\r\nimport {useEffect, useState} from \"react\";\r\n\r\nconst taskList = ({tasks,handleCheck,handleDelete}) => {\r\n\r\n  \r\n  return (\r\n    <ul>\r\n      {tasks.map((task) => (\r\n        <Task key={task.key} task={task} handleDelete={handleDelete} handleCheck={handleCheck}/>\r\n      ))}\r\n  </ul>\r\n  )\r\n}\r\n\r\nexport default taskList","C:\\Users\\maxim\\OneDrive\\Documents\\GitHub\\SwampHacks2023\\SwampHacks2023\\src\\new\\Task\\Task.js",["104","105"],[],"import React from 'react'\r\nimport {FaTrashAlt} from 'react-icons/fa';\r\nimport {useEffect, useState} from \"react\";\r\n\r\nconst Task = ({task, handleCheck, handleDelete}) => {\r\n\r\n  return (\r\n    <div>\r\n        <input\r\n            type=\"checkbox